on:
  push:
    branches: [ main ]

jobs:
  cache-test:
    runs-on: macos-12

    steps:
      - uses: actions/checkout@v2

      - name: Setup Cache
        id: setup-cache
        uses: actions/cache@v2
        with:
          path: ./output
          key: cache-2


      - name: Populate Cache
        if: steps.setup-cache.outputs.cache-hit != 'true'
        run: |
          ./go.sh
      
      - name: Read Cache
        run: |
          cat ./output/buffer

          
